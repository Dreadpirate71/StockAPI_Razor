@page "{id:int}"
@model UserDetailsModel
@{
}
<div>
    <h4>Customer Account Details</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            FirstName:
        </dt>
        <dd class="col-sm-10">
            @Model.CustomerDetails.FirstName
        </dd>
        <dt class="col-sm-2">
            LastName:
        </dt>
        <dd class="col-sm-10">
            @Model.CustomerDetails.LastName
        </dd>
        <dt class="col-sm-2">
            Address:
        </dt>
        <dd class="col-sm-10">
            @Model.CustomerDetails.Address
        </dd>
        <dt class="col-sm-2">
            City:
        </dt>
        <dd class="col-sm-10">
            @Model.CustomerDetails.City
        </dd>
        <dt class="col-sm-2">
            State:
        </dt>
        <dd class="col-sm-10">
            @Model.CustomerDetails.State
        </dd>
        <dt class="col-sm-2">
            PostalCode:
        </dt>
        <dd class="col-sm-10">
            @Model.CustomerDetails.PostalCode
        </dd>
        <dt class="col-sm-2">
            PhoneNumber:
        </dt>
        <dd class="col-sm-10">
            @Model.CustomerDetails.PhoneNumber
        </dd>
        <dt class="col-sm-2">
            Email:
        </dt>
        <dd class="col-sm-10">
            @Model.CustomerDetails.Email
        </dd>
        <dt class="col-sm-2">
            Password:
        </dt>
        <dd class="col-sm-10">
            @Model.CustomerDetails.Password
        </dd>
        <dt class="col-sm-2">
            AccountBalance:
        </dt>
        <dd class="col-sm-10">
            @Model.CustomerDetails.AccountBalance
        </dd>
    </dl>
</div>